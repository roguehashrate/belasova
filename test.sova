puts "Welcome to the test!"

fn add x y :: Int -> Int ->> Int
add x y = x + y
end

let a :: Int = 10
let b :: Int = 20
let c :: Int = add a b

puts "The sum of a and b is:"
puts c

if c == 30 then:
    puts "Correct!"
else:
    puts "Wrong!"
end

check c:
    when 10:
        puts "c is 10"
    when 30:
        puts "c is 30"
    else:
        puts "c is something else"
end
